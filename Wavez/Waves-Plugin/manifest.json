{
	"name": "Wavez Price Tracker",
	"version": "1.0",
	"description": "A plugin to track the price of audio plugins on the Waves website",
	"permissions": ["declarativeContent", "activeTab", "storage"],
	"background": {
		"scripts":["background.js"],
		"persistent": false
	},
	"manifest_version" : 2,
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"32": "images/icon.png"
		}
	},
	"icons": {
		"32": "images/icon.png"
	}
}
